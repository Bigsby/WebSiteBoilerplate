{
    "config": {
        "map": {
            "codemirror": "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.25.0"
        },
        "meta": {
            "appBuilder": {
                "deps": [
                    "angular-sanitize",
                    "angular-ui-router",
                    "codemirror/mode/javascript/javascript.min",
                    "codemirror/mode/clike/clike.min",
                    "codemirror/mode/python/python.min",
                    "codemirror/mode/mllike/mllike.min",
                    "codemirror/mode/go/go.min",
                    "codemirror/mode/ruby/ruby.min",
                    "codemirror/mode/perl/perl.min",
                    "codemirror/mode/php/php.min",
                    "codemirror/mode/vb/vb.min",
                    "codemirror/mode/powershell/powershell.min"
                ]
            }
        },
        "packages": {
            "codemirror": {
                "main": "codemirror.min.js",
                "map": {
                    "./lib/codemirror": "./codemirror.min.js"
                }
            }
        }
    },
    "styles": [
        "styles/appStyles.css",
        "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.25.0/codemirror.min.css",
        "https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.25.0/theme/neo.min.css"
    ],
    "initialData": [
        "codes"
    ],
    "theme": "ttcn"
}